datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String
  Documents  Document[]
}

model Document {
  id             Int    @id @default(autoincrement())
  title          String
  original_name  String
  topic          String
  userId         Int?
  file_size      Int
  user           User?  @relation(fields: [userId], references: [id])
  vdb            vdb[]
}

model vdb {
  id    Int @id @default(autoincrement())
  docId Int
  documents   Document @relation(fields: [docId], references: [id])
}
