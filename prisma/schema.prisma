datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  Docs  Doc[]
}

model Doc {
  id             Int    @id @default(autoincrement())
  title          String
  original_name  String
  displayed_name String
  userId         Int
  user           User   @relation(fields: [userId], references: [id])
  vdb            vdb[]
}

model vdb {
  id    Int @id @default(autoincrement())
  docId Int
  doc   Doc @relation(fields: [docId], references: [id])
}
